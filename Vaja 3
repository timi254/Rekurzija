public class Naloga3 {
    public static int zap(int n){
       if(n==1)
       {
        return 2;
       }
       else if(n==2)
       {
        return 2;
       }
       else
       {
        return(zap(n-2)*zap(n-1)-1);
       }
    }
    public static void main(String[] args) {
        System.out.println(zap(5));
    }
}
