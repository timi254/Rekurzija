public class Naloga7 {
    public static int zap(int n){
        if(n==1)
        {
            return 1;
        }
        else if(n%2==0)
        {
            return -1;
        }
        else 
        {
            return 1;
        }
    }
    public static void main(String[] args) {
        System.out.println(zap(7));
    }
}
